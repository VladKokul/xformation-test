<div class="container">
    <h2>Hover Rows</h2>
    <p>The .table-hover class enables a hover state on table rows:</p>
    <table class="table table-hover">
        <thead>
        <tr>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Email</th>
        </tr>
        </thead>
        <tbody>
{{#each model as |cont|}}
        <tr>
            <td>{{cont.nickname}}</td>
            <td>{{cont.team}}</td>
            <td>{{cont.contributions}}</td>
          <td>

          </td>
        </tr>
        {{/each}}
        </tbody>
    </table>
</div>

{{outlet}}
